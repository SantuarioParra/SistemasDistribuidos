	g++ Coordenada.cpp -c
Movimientos.o : Movimientos.cpp Movimientos.h
	g++ Movimientos.cpp -c
animacion.o : main.cpp
	g++ main.cpp -c
gfx.o: gfx.c
	gcc gfx.c -c 
